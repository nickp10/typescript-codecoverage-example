{
    "cache": false,
    "check-coverage": false,
    "extension": [
        ".ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "**/*.d.ts",
        "**/*.spec.ts",
        "src/cli.ts",
        "src/exports.ts"
    ],
    "sourceMap": true,
    "reporter": [
        "html",
        "text",
        "text-summary"
    ],
    "all": true,
    "instrument": true
}
